{
  "name": "subscribe",
  "version": "1.0.0",
  "description": "订阅管理系统",
  "private": true,
  "scripts": {
    "install:all": "npm install && npm --prefix frontend install && npm --prefix api install && npm --prefix functions install",
    "build": "npm --prefix frontend run build",
    "start": "npm --prefix frontend run preview",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:api": "npm --prefix api run dev",
    "dev:functions": "npm --prefix functions run start:dev",
    "deploy:vercel": "vercel",
    "deploy:cloudflare": "npm --prefix frontend run build && wrangler pages publish frontend/dist"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "vercel": "^32.5.0"
  }
}
